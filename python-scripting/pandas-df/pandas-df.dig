+read_into_df:
    py>: py_scripts.examples.read_td_table
    database_name: sample_datasets
    table_name: nasdaq
    docker:
        image: "digdag/digdag-python:3.7.3-stretch"
    _env:
        TD_API_KEY: ${secret:apikey}
        TD_API_SERVER: ${secret:endpoint}

+write_into_td:
    py>: py_scripts.examples.write_td_table
    database_name: pandas_test
    table_name: my_df
    docker:
        image: "digdag/digdag-python:3.7.3-stretch"
    _env:
        TD_API_KEY: ${secret:apikey}
        TD_API_SERVER: ${secret:endpoint}
