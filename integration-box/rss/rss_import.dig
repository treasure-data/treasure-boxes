_export:
  !include : config/params.yml

timezone: Asia/Tokyo

schedule:
  daily>: 02:00:00

+step1:
  docker:
    image: "armtd/td-script:2019.09"
  py>: tasks.rss_import
  dest_db: ${td.database}
  dest_table: ${td.table}
  rss_url_list: ['https://www.vogue.co.jp/rss/vogue', 'https://feeds.dailyfeed.jp/feed/s/7/887.rss']
  _env:
    td_apikey: ${secret:td.apikey}
    td_endpoint: ${secret:td.apiserver}
