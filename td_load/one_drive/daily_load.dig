timezone: UTC

schedule:
  daily>: 02:00:00

_export:
  td:
    dest_db: dest_db
    dest_table: dest_table

+prepare_table:
  td_ddl>:
  database: ${td.dest_db}
  create_tables: ["${td.dest_table}"]

+load_from_personal:
  td_load>: config/daily_load.yml
  database: ${td.dest_db}
  table: ${td.dest_table}

+load_from_business:
  td_load>: config/daily_load_business.yml
  database: ${td.dest_db}
  table: ${td.dest_table}